{
  "functions": {
    "app/api/**/*": {
      "maxDuration": 300
    },
    "app/**/*": {
      "maxDuration": 300
    }
  },

  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate=59"
        }
      ]
    },
     {
       "source": "/chat",
       "headers": [
         {
           "key": "Cache-Control",
           "value": "s-maxage=1, stale-while-revalidate=59"
         }
       ]
     }
  ]
}
